{
    "manifest_version": 2,
    "name": "Launcher Tab Closer",
    "version": "2.0.0",
  
    "description": "Close leftover tabs from Teams, Zoom and Global Protect.",
  
    "permissions": [
        "activeTab",
        "webNavigation"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "icons": {
        "16": "icons/folder16.png",
        "32": "icons/folder32.png",
        "64": "icons/folder64.png",
        "128": "icons/folder128.png"
    }
}